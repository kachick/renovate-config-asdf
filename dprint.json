{
  "json": {
    "associations": ["**/*.{json5}"]
  },
  "markdown": {},
  "typescript": {
    "quoteStyle": "preferSingle",
    "useBraces": "always",
    "module.sortImportDeclarations": "maintain",
    "module.sortExportDeclarations": "maintain",
    "importDeclaration.sortNamedImports": "maintain",
    "exportDeclaration.sortNamedExports": "maintain"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "includes": ["**/*.{json,json5,ts,cjs,js,mjs,md,yml}"],
  "excludes": [".git", "lib", "bin", ".ameba.yml"],
  "plugins": [
    "https://plugins.dprint.dev/json-0.17.2.wasm",
    "https://plugins.dprint.dev/markdown-0.15.2.wasm",
    "https://plugins.dprint.dev/typescript-0.85.0.wasm",
    "https://plugins.dprint.dev/prettier-0.24.0.json@9a57d0d8e440ad90d07a503166af47e7a6a886abd46767933f9c279f72468596"
  ]
}
