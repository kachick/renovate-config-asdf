{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Bump renovate-config-asdf itself",
  "regexManagers": [
    {
      "fileMatch": [
        "^default\\.json5?$",
        "^renovate\\.json5?$",
        "^\\.github/renovate\\.json5?$",
        "^\\.gitlab/renovate\\.json5?$",
        "^\\.renovaterc\\.json$",
        "^\\.renovaterc$"
      ],
      "matchStrings": [
        "\"github>kachick/renovate-config-asdf#(?<currentValue>[^\" \\n\\(]+)",
        "\"github>kachick/renovate-config-asdf:.*?#(?<currentValue>[^\" \\n\\(]+)",
        "\"github>kachick/renovate-config-asdf/.*?#(?<currentValue>[^\" \\n\\(]+)"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "kachick/renovate-config-asdf"
    }
  ]
}
