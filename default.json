      "matchStrings": ["(^|\\n)clojure (?<currentValue>\\S+)"],
      "depNameTemplate": "clojure/brew-install",
      "datasourceTemplate": "github-tags",
      "versioningTemplate": "regex:^(?<major>\\d+?)\\.(?<minor>\\d+?)\\.(?<patch>\\d+)\\.(?<build>\\d+)$"
