{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Bump typos in .tool-versions",
  "regexManagers": [
    {
      "fileMatch": ["\\.tool-versions$"],
      "matchStrings": ["(^|\\n)typos (?<currentValue>\\S+)"],
      // Plugin Fetcher Permalink: https://github.com/plugin-author/asdf-typos/blob/740c457679756d679ba115d2dda0265a824d63af/bin/list-all#L42
      // DataSource URL: https://github.com/owner/typos/tags
      "depNameTemplate": "owner/typos",
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^typos-v(?<version>\\S+?)-release"
    }
  ]
}
